{
  "name": "@futengda/react-components",
  "version": "1.0.2",
  "description": "React components based on Antd",
  "module": "./components/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "prettier -c --write **/*",
    "lint-staged": "lint-staged",
    "tsc": "tsc",
    "dist": "antd-tools run dist",
    "compile": "antd-tools run compile",
    "clean": "antd-tools run clean",
    "start": "antd-tools run start",
    "site": "antd-tools run site",
    "deploy": "antd-tools run update-self && antd-tools run deploy",
    "just-deploy": "antd-tools run just-deploy",
    "pub": "antd-tools run update-self && antd-tools run pub",
    "prepublish": "antd-tools run guard"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@v9.git.n.xiaomi.com:futengda/react-components.git"
  },
  "keywords": [
    "react",
    "component"
  ],
  "author": "futengda@xiaomi.com",
  "license": "MIT",
  "dependencies": {
    "antd": "^3.19.8",
    "antd-tools": "^7.6.0",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^6.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-tinker": "^0.5.0",
    "immutability-helper": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.1",
    "stylelint-config-prettier": "^5.2.0"
  }
}
